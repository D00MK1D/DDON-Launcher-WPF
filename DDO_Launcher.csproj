<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net9.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Images\btnClose.png" />
    <None Remove="Images\btnClose.svg" />
    <None Remove="Images\btnGeneralSettings.png" />
    <None Remove="Images\btnGeneralSettings.svg" />
    <None Remove="Images\btnMinimize.png" />
    <None Remove="Images\btnMinimize.svg" />
    <None Remove="Images\btnServerSettings.png" />
    <None Remove="Images\btnServerSettings.svg" />
  </ItemGroup>

  <ItemGroup>
    <Page Include="ModSettingsWindow" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Fonts\EBGaramond-Regular.ttf" />
    <Resource Include="Fonts\Jersey25-Regular.ttf" />
    <Resource Include="Images\btnGeneralSettings.png" />
    <Resource Include="Images\logo.png" />
  </ItemGroup>

</Project>
