<Window x:Name="LauncherWindow" x:Class="DDO_Launcher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DDO_Launcher"
        mc:Ignorable="d"
        Title="Dragon's Dogma Online Launcher" Height="500" Width="1000"
        WindowStyle="None"
        ResizeMode="NoResize"
        Background="Transparent"
        AllowsTransparency="True">


    <Border x:Name="LauncherBorder" IsHitTestVisible="True"
            CornerRadius="8">
        <Border.Background>
            <ImageBrush ImageSource="{Binding CustomBackground}" Stretch="UniformToFill" TileMode="None"/>
        </Border.Background>

        <Grid x:Name="LauncherGrid">
            <Grid.RowDefinitions>
                <RowDefinition Height="40"/>
                <RowDefinition Height="109"/>
                <RowDefinition Height="351"/>
            </Grid.RowDefinitions>

            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="301*"/>
                <ColumnDefinition Width="699*"/>
            </Grid.ColumnDefinitions>

            <Border Grid.ColumnSpan="2" 
                    CornerRadius="8" 
                    HorizontalAlignment="Left" 
                    Height="500" Grid.RowSpan="3" 
                    VerticalAlignment="Top" 
                    Width="1000" Background="#B2191A19"
                    MouseLeftButtonDown="DragWindow_MouseDown"/>

            <Border Background="#E5191A19"
                    Margin="10,3,20,20"
                    CornerRadius="8"
                    Grid.Column="1"
                    Grid.RowSpan="2" Grid.Row="1">
                <Image x:Name="imgHighlight" 
                       IsHitTestVisible="False" 
                       VerticalAlignment="Top" 
                       Height="120" 
                       Stretch="UniformToFill" 
                       Source="/Images/newsbanner.jpg" 
                       Margin="2,2,2,0">
                    <Image.Clip>
                        <RectangleGeometry Rect="0,0,665,130" RadiusX="8" RadiusY="8"/>
                    </Image.Clip>
                </Image>
            </Border>

            <Border x:Name="Form"
                    Background="#E5191A19"
                    Margin="20,20,10,20"
                    CornerRadius="8"
                    Grid.Column="0"
                    Grid.Row="2">

                <Grid Margin="10,10,10,10">

                    <TextBox x:Name="textAccount" 
                             VerticalAlignment="Top" 
                             TextWrapping="NoWrap" 
                             VerticalContentAlignment="Center"
                             AcceptsReturn="False"
                             AllowDrop="False" 
                             TabIndex="1" 
                             Height="24" 
                             BorderBrush="{x:Null}" 
                             Foreground="#D3D3D3" 
                             Margin="10,24,10,0" 
                             FontFamily="/Fonts/#EB Garamond" 
                             Text="" FontSize="16" 
                             MaxLength="100" MaxLines="1" 
                             BorderThickness="0,0,0,0" 
                             VerticalScrollBarVisibility="Disabled" 
                             UndoLimit="10"
                             Style="{StaticResource StylshTextBox}">

                        <TextBox.Resources>
                            <Style TargetType="{x:Type Border}">
                                <Setter Property="CornerRadius" Value="2"/>
                            </Style>
                        </TextBox.Resources>

                        <TextBox.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#494949"/>
                                <GradientStop Color="#1A1A1A" Offset="1"/>
                            </LinearGradientBrush>
                        </TextBox.Background>
                    </TextBox>

                    <PasswordBox x:Name="textPassword" 
                                 TabIndex="2" 
                                 AllowDrop="False" 
                                 VerticalAlignment="Top" 
                                 VerticalContentAlignment="Center"
                                 Height="24" 
                                 BorderBrush="{x:Null}" 
                                 PasswordChar="•"
                                 Foreground="#D3D3D3" 
                                 Margin="10,74,10,0" 
                                 FontFamily="/Fonts/#EB Garamond" 
                                 FontSize="16" 
                                 MaxLength="100" 
                                 BorderThickness="0,0,0,0">

                        <PasswordBox.Resources>
                            <Style TargetType="{x:Type Border}">
                                <Setter Property="CornerRadius" Value="2"/>
                            </Style>
                        </PasswordBox.Resources>

                        <PasswordBox.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#494949"/>
                                <GradientStop Color="#1A1A1A" Offset="1"/>
                            </LinearGradientBrush>
                        </PasswordBox.Background>

                    </PasswordBox>

                    <Button x:Name="btnSubmit" 
                            Content="Login" 
                            Margin="10,207,10,0" TabIndex="4" 
                            Click="btnSubmit_Click" 
                            FontFamily="/Fonts/#EB Garamond" 
                            BorderThickness="0,0,0,0" 
                            FontSize="26" Height="43"
                            VerticalAlignment="Top"
                            Style="{StaticResource GoldenButton}"/>

                    <Button x:Name="btnChangeAction" 
                            Content="Register" 
                            Margin="10,259,10,0" TabIndex="5" 
                            Click="btnChangeAction_Click" 
                            FontFamily="/Fonts/#EB Garamond" 
                            BorderThickness="0,0,0,0" 
                            Foreground="#FFB1B1B1"
                            FontSize="15"
                            Height="24" 
                            VerticalAlignment="Top">


                        <Button.Style>
                            <Style TargetType="{x:Type Button}">
                                <Setter Property="Template">
                                    <Setter.Value>
                                        <ControlTemplate TargetType="{x:Type Button}">
                                            <Border x:Name="border"
                                                    Background="{TemplateBinding Background}"
                                                    CornerRadius="2"
                                                    BorderThickness="0"
                                                    BorderBrush="Gray">
                                                <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                            </Border>
                                            <ControlTemplate.Triggers>
                                                <Trigger Property="IsMouseOver" Value="True">
                                                    <Setter TargetName="border" Property="Background">
                                                        <Setter.Value>
                                                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                                <GradientStop Color="#FF5A5A5A"/>
                                                                <GradientStop Color="#FF3F3F3F" Offset="1"/>
                                                            </LinearGradientBrush>
                                                        </Setter.Value>
                                                    </Setter>
                                                </Trigger>

                                                <Trigger Property="IsPressed" Value="True">
                                                    <Setter TargetName="border" Property="Background">
                                                        <Setter.Value>
                                                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                                <GradientStop Color="#FF2A2A2A"/>
                                                                <GradientStop Color="#FF1A1A1A" Offset="1"/>
                                                            </LinearGradientBrush>
                                                        </Setter.Value>
                                                    </Setter>
                                                </Trigger>
                                            </ControlTemplate.Triggers>
                                        </ControlTemplate>
                                    </Setter.Value>
                                </Setter>

                                <Setter Property="Background">
                                    <Setter.Value>
                                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                            <GradientStop Color="#FF494949"/>
                                            <GradientStop Color="#FF1A1A1A" Offset="1"/>
                                        </LinearGradientBrush>
                                    </Setter.Value>
                                </Setter>
                            </Style>
                        </Button.Style>
                    </Button>

                    <Label x:Name="labelAccount" 
                           Content="Account Name" 
                           Margin="6,-2,14,266"/>

                    <Label x:Name="passLabel" 
                           Content="Password" 
                           Margin="6,48,14,216"/>

                    <Label x:Name="labelServer" 
                           Content="Server" 
                           Margin="6,98,14,0" 
                           Height="28" 
                           VerticalAlignment="Top"/>

                    <ComboBox x:Name="serverComboBox"
                              FontFamily="/Fonts/#EB Garamond"
                              BorderThickness="0"
                              Foreground="#FFB1B1B1"
                              TabIndex="3"
                              Margin="10,124,43,0"
                              Height="24"
                              VerticalAlignment="Top"
                              SelectionChanged="serverComboBox_SelectionChanged"
                              Style="{StaticResource MainCB}">


                    </ComboBox>

                    <Button x:Name="btnServerSettings" 
                            Margin="218,124,10,0" TabIndex="5" 
                            FontFamily="Noto Serif" 
                            BorderThickness="0,0,0,0" Foreground="LightGray" Click="btnServerSettings_Click" Height="24" VerticalAlignment="Top">

                        <Button.Style>
                            <Style TargetType="{x:Type Button}">
                                <Setter Property="Template">
                                    <Setter.Value>
                                        <ControlTemplate TargetType="{x:Type Button}">
                                            <Border x:Name="border"
                                                    Background="{TemplateBinding Background}"
                                                    CornerRadius="2"
                                                    BorderThickness="0"
                                                    BorderBrush="Gray">
                                                <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                            </Border>
                                            <ControlTemplate.Triggers>
                                                <Trigger Property="IsMouseOver" Value="True">
                                                    <Setter TargetName="border" Property="Background">
                                                        <Setter.Value>
                                                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                                <GradientStop Color="#FF5A5A5A"/>
                                                                <GradientStop Color="#FF3F3F3F" Offset="1"/>
                                                            </LinearGradientBrush>
                                                        </Setter.Value>
                                                    </Setter>
                                                </Trigger>

                                                <Trigger Property="IsPressed" Value="True">
                                                    <Setter TargetName="border" Property="Background">
                                                        <Setter.Value>
                                                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                                <GradientStop Color="#FF2A2A2A"/>
                                                                <GradientStop Color="#FF1A1A1A" Offset="1"/>
                                                            </LinearGradientBrush>
                                                        </Setter.Value>
                                                    </Setter>
                                                </Trigger>
                                            </ControlTemplate.Triggers>
                                        </ControlTemplate>
                                    </Setter.Value>
                                </Setter>

                                <Setter Property="Background">
                                    <Setter.Value>
                                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                            <GradientStop Color="#FF494949"/>
                                            <GradientStop Color="#FF1A1A1A" Offset="1"/>
                                        </LinearGradientBrush>
                                    </Setter.Value>
                                </Setter>
                            </Style>
                        </Button.Style>
                        <Path Fill="#FFB1B1B1" Data="M17.907 4.93c.104-.105.294-.186.539-.18a.85.85 0 0 1 .572.216c.13.13.222.34.231.575s-.065.413-.167.515l-7.49 7.5l-1.273.116l.1-1.245zm2.173-1.024c-.437-.438-1.037-.643-1.6-.656s-1.182.167-1.635.62l-7.682 7.692a.75.75 0 0 0-.217.47l-.194 2.407a.75.75 0 0 0 .816.807l2.43-.22a.75.75 0 0 0 .463-.218l7.682-7.692c.456-.456.627-1.073.605-1.635a2.37 2.37 0 0 0-.668-1.575M5.812 4.75a2.57 2.57 0 0 0-2.563 2.562v10.875a2.57 2.57 0 0 0 2.563 2.563h10.875a2.57 2.57 0 0 0 2.563-2.562V12.75a.75.75 0 0 0-1.5 0v5.438c0 .582-.48 1.062-1.063 1.062H5.812c-.583 0-1.063-.48-1.063-1.062V7.312c0-.582.48-1.062 1.063-1.062h5.437a.75.75 0 0 0 0-1.5z" Stretch="Fill" Width="13" Height="13"/>
                    </Button>

                    <CheckBox x:Name="cbRemember" 
                              Content="CheckBox" 
                              Margin="10,168,221,0"
                              VerticalAlignment="Top"
                              Foreground="White"
                              BorderBrush="{x:Null}"
                              OpacityMask="White"
                              IsChecked="False" 
                              IsTabStop="False" 
                              Focusable="False" 
                              RenderTransformOrigin="0.5,0.5" 
                              Visibility="Visible"/>

                    <Label x:Name="labelRemember" 
                           Content="Remember my login details" 
                           Margin="30,163,-14,99" 
                           FontSize="14"/>

                    <Label x:Name="labelEmail" 
                           Content="E-mail" 
                           Margin="6,98,10,0" 
                           FontFamily="/Fonts/#EB Garamond" 
                           FontSize="14" 
                           Height="32" 
                           VerticalAlignment="Top" 
                           Foreground="#FFB1B1B1" 
                           Visibility="Hidden"/>

                    <TextBox x:Name="textEmail" 
                             TabIndex="2" 
                             AllowDrop="False" 
                             TextWrapping="NoWrap" 
                             VerticalContentAlignment="Center"
                             AcceptsReturn="False"
                             VerticalAlignment="Top" 
                             Height="24" 
                             BorderBrush="{x:Null}" 
                             Foreground="#D3D3D3" 
                             Margin="10,124,10,0" 
                             FontFamily="/Fonts/#EB Garamond" 
                             FontSize="16" 
                             MaxLength="100" 
                             MaxLines="1" 
                             BorderThickness="0,0,0,0" 
                             VerticalScrollBarVisibility="Disabled" 
                             UndoLimit="10" Visibility="Hidden">

                        <TextBox.Resources>
                            <Style TargetType="{x:Type Border}">
                                <Setter Property="CornerRadius" Value="2"/>
                            </Style>
                        </TextBox.Resources>

                        <TextBox.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#494949"/>
                                <GradientStop Color="#1A1A1A" Offset="1"/>
                            </LinearGradientBrush>
                        </TextBox.Background>
                    </TextBox>
                </Grid>
            </Border>


            <Grid Grid.Row="2" Grid.Column="1" Margin="34,36,36,10">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>



                <StackPanel Grid.Row="0" Orientation="Horizontal" Margin="0,4,0,4.3">
                    <Border Style="{StaticResource MaintenanceBorderStyle}" Background="#FF275F28">
                        <TextBlock Style="{StaticResource MaintenanceTextStyle}" Text="EVENT"/>
                    </Border>
                    <Border CornerRadius="4" Padding="0,2,6,2">
                        <TextBlock Style="{DynamicResource tagDateTextBlock}" Text="12/12/2012" />
                    </Border>
                </StackPanel>

                <TextBlock Grid.Row="1" 
                           Text="Monster Hunter Collaboration has begun!"
                           Margin="0,4,0,4" Style="{DynamicResource HeadlineTextBlock}"/>
                <TextBlock Grid.Row="2"
                           Text="The collaboration evenLorem ipsum dolor sit amet, consectetur adipiscing elit. Vestibulum purus ex, accumsan et imperdiet nec, sodales vitae est. Praesent consectetur lectus at lorem consectetur, sed fermentum nunc tincidunt. Suspendisse quis mattis nibh. Fusce ut dignissim magna. Ut vel mollis mi, id vestibulum lectus. Aliquam in dignissim risus. Suspendisse tincidunt auctor ante sit amet sodales. Aenean id ante id massa fringilla vestibulum non quis mi. Nam turpis mi, commodo sit amet orci eget, auctor accumsan odio. Cras sodales, massa a molestie condimentum, metus risus blandit velit, non dapibus elit enim eget augue. In hac habitasse platea dictumst.t starts this week! Get ready for exclusive quests and rewards."
                           TextWrapping="Wrap" Style="{DynamicResource EventDetailsTextBlockStyle}" Padding="0,0,12,12"/>
            </Grid>
            <Image Source="{Binding CustomLogo}" Margin="20,0,20,0" Grid.Row="1" IsHitTestVisible="False"/>
            
            <Grid Grid.Row="0" Grid.ColumnSpan="2">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition/>
                    <ColumnDefinition Width="Auto" MinWidth="700"/>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>

                <Button x:Name="btnClose"
            Grid.Row="0"
            Grid.Column="1"
            Margin="655,10,20,11"
            Click="btnClose_Click">

                    <Button.Resources>
                        <Style TargetType="{x:Type Path}">
                            <Setter Property="Fill" Value="#D3D3D3"/>
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding IsMouseOver, RelativeSource={RelativeSource AncestorType={x:Type Button}}}" Value="True">
                                    <Setter Property="Fill" Value="White"/>
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </Button.Resources>

                    <Button.Style>
                        <StaticResource ResourceKey="IconButtonStyle"/>
                    </Button.Style>

                    <Path Stretch="Uniform"
              Height="16" Width="16"
              Data="M12.75 11.25V5a.75.75 0 0 0-1.5 0v6.25H5a.75.75 0 0 0 0 1.5h6.25V19a.76.76 0 0 0 .75.75a.75.75 0 0 0 .75-.75v-6.25H19a.75.75 0 0 0 .75-.75a.76.76 0 0 0-.75-.75Z"
              RenderTransformOrigin="0.5,0.5">
                        <Path.RenderTransform>
                            <RotateTransform Angle="-45"/>
                        </Path.RenderTransform>
                    </Path>
                </Button>

                <Button x:Name="btnModinstall" Grid.Row="0"
            Grid.Column="1"
            ToolTip="Mod Install"
            Margin="581,10,94,11"
            Background="Transparent"
            BorderThickness="0" Click="btnInstallMod_Click" Cursor="Hand">

                    <Button.Resources>
                        <Style TargetType="{x:Type Path}">
                            <Setter Property="Fill" Value="#D3D3D3"/>
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding IsMouseOver, RelativeSource={RelativeSource AncestorType={x:Type Button}}}" Value="True">
                                    <Setter Property="Fill" Value="White"/>
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </Button.Resources>

                    <Button.Style>
                        <StaticResource ResourceKey="IconButtonStyle"/>
                    </Button.Style>
                    <Path Fill="#D3D3D3" Stretch="Uniform" Width="18" Data="M25 3.5a1 1 0 1 0-2 0V7h-3.5a1 1 0 1 0 0 2H23v3.5a1 1 0 1 0 2 0V9h3.5a1 1 0 1 0 0-2H25zM5 6.25V15h10V6.25C15 5.56 14.44 5 13.75 5h-7.5C5.56 5 5 5.56 5 6.25m12 0V15h8.75A3.25 3.25 0 0 1 29 18.25v7.5A3.25 3.25 0 0 1 25.75 29H6.25A3.25 3.25 0 0 1 3 25.75V6.25A3.25 3.25 0 0 1 6.25 3h7.5A3.25 3.25 0 0 1 17 6.25M5 17v8.75c0 .69.56 1.25 1.25 1.25H15V17zm12 0v10h8.75c.69 0 1.25-.56 1.25-1.25v-7.5c0-.69-.56-1.25-1.25-1.25z"/>
                </Button>

                <Button x:Name="btnMinimize"
            Grid.Row="0"
            Grid.Column="1"
            Margin="625,10,50,11"
            Click="btnMinimize_Click">

                    <Button.Resources>
                        <Style TargetType="{x:Type Path}">
                            <Setter Property="Fill" Value="#D3D3D3"/>
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding IsMouseOver, RelativeSource={RelativeSource AncestorType={x:Type Button}}}" Value="True">
                                    <Setter Property="Fill" Value="White"/>
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </Button.Resources>

                    <Button.Style>
                        <StaticResource ResourceKey="IconButtonStyle"/>
                    </Button.Style>

                    <Path Stretch="Uniform"
              Width="14"
              Data="M20 13H4a1 1 0 0 1 0-2h16a1 1 0 0 1 0 2"
              RenderTransformOrigin="0.495,0.513"/>
                </Button>

                <Button x:Name="btnTranslation" Grid.Row="0"
            Grid.Column="1"
            ToolTip="Install Translation"
            Margin="547,10,128,11"
            Background="Transparent"
            BorderThickness="0" Click="btnInstallTranslation_Click">

                    <Button.Resources>
                        <Style TargetType="{x:Type Path}">
                            <Setter Property="Fill" Value="#D3D3D3"/>
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding IsMouseOver, RelativeSource={RelativeSource AncestorType={x:Type Button}}}" Value="True">
                                    <Setter Property="Fill" Value="White"/>
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </Button.Resources>

                    <Button.Style>
                        <StaticResource ResourceKey="IconButtonStyle"/>
                    </Button.Style>
                    <Path Stretch="Uniform" Width="18" Data="M10.528 1.015a.5.5 0 0 0-.058.999A7.97 7.97 0 0 1 15.292 4H14.5a.5.5 0 0 0 0 1h2a.5.5 0 0 0 .5-.5v-2a.5.5 0 0 0-1 0v.792a8.97 8.97 0 0 0-5.472-2.277M5.5 16h-.792a7.97 7.97 0 0 0 4.82 1.986a.5.5 0 0 1-.058.999A8.97 8.97 0 0 1 4 16.708v.792a.5.5 0 1 1-1 0v-2a.5.5 0 0 1 .5-.5h2a.5.5 0 0 1 0 1M6.92 4.216a5.04 5.04 0 0 0-3.679.329a4 4 0 0 0-.297.168l-.02.013l-.006.004l-.002.001l-.002.002a.5.5 0 0 0 .566.824h-.001l.007-.004l.038-.024q.056-.033.17-.093a4.04 4.04 0 0 1 2.947-.26c.712.21 1.026.593 1.186.985c.155.38.176.797.178 1.172c-.543-.149-1.212-.296-1.921-.325c-1.036-.043-2.197.163-3.207 1.003c-.888.74-1.094 1.871-.807 2.842a2.97 2.97 0 0 0 2.248 2.082c.906.188 1.862-.07 2.642-.414a8 8 0 0 0 1.046-.554v.533a.5.5 0 1 0 1 0V7.445c0-.392 0-1.04-.253-1.661c-.272-.668-.82-1.272-1.832-1.569m1.086 4.156v2.385a7.6 7.6 0 0 1-1.448.849c-.705.31-1.43.476-2.037.35A1.97 1.97 0 0 1 3.03 10.57c-.194-.658-.041-1.35.488-1.79c.763-.634 1.652-.81 2.525-.773c.722.03 1.406.202 1.964.365m6.16-2.343a.5.5 0 0 1 .304.638c-.12.341-.252.81-.353 1.194a23 23 0 0 0 1.77-.348a.5.5 0 0 1 .225.974c-.774.18-1.502.326-2.24.416q-.144.68-.227 1.247A5.4 5.4 0 0 1 15 10.02V10a.5.5 0 0 1 1 0q0 .085-.002.169c1.142.31 1.98 1.071 2.346 2.057c.614 1.66-.215 3.653-2.64 4.731a.5.5 0 1 1-.407-.914c2.074-.921 2.495-2.428 2.11-3.47c-.22-.589-.729-1.124-1.494-1.386c-.23 1.537-.892 2.76-1.684 3.606c.084.173.167.35.236.522a.5.5 0 1 1-.93.37a5 5 0 0 0-.087-.205c-.42.3-.857.507-1.277.604c-.576.132-1.208.068-1.637-.39c-.644-.689-.69-1.833-.248-2.853c.38-.876 1.133-1.723 2.3-2.3c.053-.468.134-.982.248-1.552c-.441.019-.893.014-1.368-.018a.5.5 0 1 1 .068-.997c.53.035 1.03.034 1.523.002a20 20 0 0 1 .472-1.643a.5.5 0 0 1 .638-.304M12.5 11.74c-.657.441-1.074.981-1.297 1.498c-.35.805-.215 1.477.06 1.772c.094.1.299.186.683.098c.311-.072.684-.252 1.066-.551a6 6 0 0 1-.381-1.205a7.5 7.5 0 0 1-.13-1.612m1.28 2.039c.535-.684.978-1.613 1.147-2.763c-.389-.01-.816.04-1.282.162l-.12.043c-.06.833-.011 1.45.086 1.935c.046.23.104.434.17.623"/>
                </Button>
            </Grid>
        </Grid>
    </Border>
</Window>
